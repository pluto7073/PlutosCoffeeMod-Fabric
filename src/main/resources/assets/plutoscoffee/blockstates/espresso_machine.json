        "has_espresso": "true",
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "plutoscoffee:block/espresso_machine_cups",
        "y": 180
      },
      "when": {
        "has_espresso": "true",
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "plutoscoffee:block/espresso_machine_cups",
        "y": 90
      },
      "when": {
        "has_espresso": "true",
        "facing": "east"
      }
